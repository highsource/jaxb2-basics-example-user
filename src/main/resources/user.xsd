<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:com="http://example.com/Common"
           xmlns="http://example.com/User"
           targetNamespace="http://example.com/User">
    <xs:import namespace="http://example.com/Common" schemaLocation="http://example.com/Common.xsd"/>

    <xs:element name="box">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="com:item" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
